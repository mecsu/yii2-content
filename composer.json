{
  "name": "mecsu/yii2-content",
  "description": "Content manager",
  "keywords": ["yii2", "yii2-content", "content", "manager"],
  "type": "yii2-extension",
  "license": "MIT",
  "version": "1.1.2",
  "authors": [
    {
      "name": "Xuan Nguyen",
      "email": "xuanhn.vt@gmail.com"
    }
  ],
  "repositories":[
    {
        "type": "git",
        "url": "https://github.com/mecsu/yii2-content"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.40",
    "wdmg/yii2-base": "^1.3.0",
    "wdmg/yii2-selectinput": "^1.0.9",
    "wdmg/yii2-widgets": "^1.0.4"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "mecsu\\content\\": ""
    }
  },
  "extra": {
    "options": {
      "routePrefix": "admin"
    }
  }
}